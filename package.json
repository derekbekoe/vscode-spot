{
    "name": "vscode-spot",
    "description": "VS Code extension for the Spot service.",
    "version": "0.0.1",
    "publisher": "derek@derekbekoe.com",
    "repository": {
        "url": "https://github.com/derekbekoe/vscode-spot.git"
    },
    "homepage": "https://github.com/derekbekoe/vscode-spot/blob/master/README.md",
    "license": "SEE LICENSE IN LICENSE.md",
    "aiKey": "14c91bb7-84bd-43e2-80f0-1977e0bc7e49",
    "preview": true,
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:spotExplorer",
        "onCommand:spot.Create",
        "onCommand:spot.Connect",
        "onCommand:spot.Disconnect",
        "onCommand:spot.Terminate"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "views": {
            "explorer": [
                {
					"id": "spotExplorer",
					"name": "Spot Workspace",
					"when": "showSpotExplorer"
				}
            ]
        },
        "commands": [
            {
                "command": "spot.Create",
                "title": "Create",
                "category": "Spot"
            },
            {
                "command": "spot.Connect",
                "title": "Connect",
                "category": "Spot"
            },
            {
                "command": "spot.Disconnect",
                "title": "Disconnect",
                "category": "Spot"
            },
            {
                "command": "spot.Terminate",
                "title": "Terminate",
                "category": "Spot"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "lint": "tslint --project tsconfig.json -e src/*.d.ts -t verbose",
        "test": "node ./node_modules/vscode/bin/test",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "tslint": "^5.7.0",
        "tslint-microsoft-contrib": "5.0.3",
        "typescript": "^2.7.2",
        "vscode": "^1.1.10",
        "mocha": "^5.0.1",
        "@types/node": "^9.4.6",
        "@types/mocha": "^2.2.48",
        "@types/request-promise": "^4.1.41",
        "@types/ws": "^4.0.1"
    },
    "dependencies": {
        "request-promise": "^4.2.2",
        "vscode-extension-telemetry": "^0.0.15",
        "ws": "^5.0.0"
    }
}
